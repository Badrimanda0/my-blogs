4
// app/page.tsx
import Image from "next/image";

export default function Services() {
  const services = [
    { title: "Product Development", img: "/service1.jpg" },
    { title: "Software Testing", img: "/service2.jpg" },
    { title: "IT Consulting", img: "/service3.jpg" },
    { title: "IT Services", img: "/service4.jpg" },
    { title: "Staffing Solutions", img: "/service5.jpg" },
  ];

  return (
    <main className="px-6 py-12">
      {/* Title */}
      <h2 className="text-2xl font-bold text-center text-blue-700 mb-8">
        Our Services
      </h2>

      {/* First Row - 3 items side by side */}
      <div className="flex justify-center gap-6 mb-6">
        {services.slice(0, 3).map((service, index) => (
          <div
            key={index}
            className="relative w-72 h-56 rounded-lg overflow-hidden shadow-lg"
          >
            <Image
              src={service.img}
              alt={service.title}
              layout="fill"
              objectFit="cover"
              className="rounded-lg"
            />
            <div className="absolute bottom-0 w-full bg-black bg-opacity-40 text-white text-lg font-semibold p-3 text-center">
              {service.title}
            </div>
          </div>
        ))}
      </div>

      {/* Second Row - 2 items side by side, centered */}
      <div className="flex justify-center gap-6">
        {services.slice(3).map((service, index) => (
          <div
            key={index}
            className="relative w-72 h-56 rounded-lg overflow-hidden shadow-lg"
          >
            <Image
              src={service.img}
              alt={service.title}
              layout="fill"
              objectFit="cover"
              className="rounded-lg"
            />
            <div className="absolute bottom-0 w-full bg-black bg-opacity-40 text-white text-lg font-semibold p-3 text-center">
              {service.title}
            </div>
          </div>
        ))}
      </div>
    </main>
  );
}
